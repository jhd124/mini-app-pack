{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "outDir": "dist",
        "downlevelIteration": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules",
    ],
    "compileOnSave": true
}